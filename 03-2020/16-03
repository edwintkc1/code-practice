// 洛谷 新手村
// P1425
// input time, output their difference. The first two input is hh:mm, the last two is also hh:mm, assume the time of last two
// must larger than first two
// input:12 50 19 10
// output:6 20

#include <iostream>
using namespace std;

int main()
{
    int a,b,c,d,hour,min;
    cin>>a>>b>>c>>d;
    a = a*60+b;          //change hour to minutes, then no need to consider negative case
    c = c*60+d;
    hour = (c-a)/60;
    min = (c-a)%60;
    cout<<hour<<" "<<min<<endl;
}
