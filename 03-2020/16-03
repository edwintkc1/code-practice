// 16-03-2020
// 洛谷 初入洛谷
// P1116
// input size of array, input data of array[i], count how many times to move if I need to sort the array by ascending order.
// input: 4
//        4 3 2 1
// output:6 

#include <iostream>
#include <cstdio>
using namespace std;
int n, sum;
int main()
{
    cin>>n;
    int a[10000];
    for(int i=0;i<n;i++)
        cin>>a[i];
    for(int i=0;i<n-1;i++)
        for(int j=0;j<n-i-1;j++)
            if(a[j]>a[j+1]){
                swap(a[j],a[j+1]);
                sum++;
            }
    cout<< sum <<endl;
    return 0;
}
