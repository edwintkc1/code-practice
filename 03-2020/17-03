//洛谷 新手村
// #7
// P1035
// Sn = 1 + 1/2 + 1/3 +...+1/n, now input a integer k , ouput a smallest n that let Sn>k
// input: 1
// output:2

#include <iostream>
using namespace std;

int main()
{
	int k;
	double sum = 0;
	cin >> k;
	for (double n =1;  n<= 1000000; n++) {
		sum += 1.0/ n;     
		if (sum > k) {
			cout << n << endl;
			break;
		}
	}
	return 0;
}
