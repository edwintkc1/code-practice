#include <iostream>
using namespace std;

int findChar(char input[], char key[],int length1, int length2) {
	int result = 0, j = 0;
	bool flag = false;
		for(int i =0;i<length1;i++){
			if (input[i] == key[j]){
				flag = true;
				result = i;
				j++;
				if (input[i] != key[j]) {
					flag = false;
				}
			}
			else
				return -1;
	}

}

int main() {
	char input[50], key[50];
	cin >> input>>key;
	int length1 = 0, length2 = 0;

	while (input[length1] != '\0') {
		length1++;
	}
	while (input[length2] != '\0') {
		length2++;
	}


	int ans = findChar(input,key,length1,length2);
	cout << ans;
	return 0;
}
