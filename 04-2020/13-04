// codeforces 116A
// #46
// tram. input n stops. each stop ppl can get into the tram and exit the tram.
// ppl always exit first.
// you need to ouput the maximum capacity of the tram
// input: 4
	  0 3
	  2 5
	  4 2
	  4 0
// output:6

#include <iostream>
using namespace std;

int main() {
	int n, exit, enter, ppl = 0, max = 0;
	cin >> n;
	while (n--) {
		cin >> exit >> enter;
		ppl = ppl - exit + enter;
		if (ppl > max)
			max = ppl;
	}
	cout << max;
	return 0;
}
----------------------------------------------------------------------------------------------------------------------
// codeforces 339A
// #47
// input a simple summation equation with no order, output it with ascending order.
// input: 1+1+3+1+3
// output:1+1+1+3+3

// 1.用string接收，再整個int array擺翻曬d numbers落去
// 2.sort int array
// 3.再擺翻int array d numbers落string

#include <iostream>
#include<algorithm>
using namespace std;

int main() {
	string s;
	cin >> s;
	int a[101];
	for (int i = 0; i < 101; i++)
		a[i] = INT_MAX;
	for (int i = 0; i < s.length(); i++) {
		if (s[i] == '+')
			continue;
		a[i] = s[i] - '0';
	}
	sort(a, a + 101);
	int index = 0;
	for (int i = 0; i < s.length(); i++) {
		if (s[i] == '+')
			continue;
		s[i] = a[index] + '0';
		index++;
	}
	cout << s;
	return 0;
}

// 改良後：
// 1.bubble sort, compare s[j] and s[j+1]
// 2.need to prevent out of range, so when j == the last index of string, break it

#include <iostream>
using namespace std;

int main() {
	string s;
	cin >> s;
	for (int i = 0; i < s.length(); i += 2)
		for (int j = 0; j < s.length() - i - 2; j += 2) {
			if (j == s.length()-1)
				break;
			if (s[j] > s[j + 2])
				swap(s[j], s[j + 2]);
		}
	cout << s;
	
	return 0;
}
