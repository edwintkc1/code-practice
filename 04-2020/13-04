// codeforces 126B
// #43
// input n drinks, and their volume, ouput the average volume.
// input: 3
          50 50 100
// output:66.666666666667

// care about the precision
// we could simply use while(n--), no need new a array

#include <iostream>
#include<iomanip>
using namespace std;

int main() {
	int n, t, replace_n;
	cin >> n;
	replace_n = n;
	double sum = 0;
	while (n--) {
			cin >> t;
			sum += t;
	}
	sum = sum / replace_n;
	cout << fixed<<setprecision(12)<<sum;
	return 0;
}
----------------------------------------------------------------------------------------------------------------------
// codeforces 248A
// #44
// input integer n , it represent number of array[2], each data in column contains 0 or 1.
// You need to change element in the same row, to make the row become the same.
// You need to take the least step.
// input: 5
	  0 1
	  1 0
	  0 1
	  1 1
	  0 1
// output:3

// in the row of the example, turn two 1 into 0 (2 steps), turn one 0 into 1 ( 1 step), total 3 steps.
// Dont need a array acutally
// use variable left and right to store, and use count0_left and count0_right to count the total number of 0.
// if n - count0_left is larger than count0_left, indicates that we need to change the 0, cuz it will take fewer steps.
// So do the right part.

#include <iostream>
#include <algorithm>
using namespace std;

int main() {
	int n, left, right;
	int count0_left = 0, count0_right = 0;
	int ans = 0;
	cin >> n;
	for (int i = 0; i < n; i++) {
		cin >> left >> right;
		if (left == 0)
			count0_left++;
		if (right == 0)
			count0_right++;
	}
	ans = min(count0_left, n - count0_left) + min(count0_right, n - count0_right);
	cout << ans;

	return 0;
}

----------------------------------------------------------------------------------------------------------------------
// codeforces 155A
// #45
// input n number, if any number is larger than current max, or lesser than current min
// ans++
// input: 10
	  4664 6496 5814 7010 5762 5736 6944 4850 3698 7242
// output:4

#include <iostream>
using namespace std;

int main() {
	int n, x, max, min, ans = 0;
	cin >> n >> x;
	max = x, min = x;
	n -= 1;
	while (n--) {
		cin >> x;
		if (max > x) {
			max = x;
			ans++;
		}
		if (min < x) {
			min = x;
			ans++;
		}
	}
	cout << ans;
	return 0;
}

